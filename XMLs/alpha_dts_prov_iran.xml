			</column>

			<!-- START Discrete Traits Model                                             -->
			<column label="reg.nonZeroRates" dp="0" width="6">
				<sumStatistic idref="reg.nonZeroRates"/>
			</column>

			<!-- END Discrete Traits Model                                               -->

		</log>
		

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="50000" fileName="alpha_dis_iran_prov.log" overwrite="false">
			<joint idref="joint"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<tmrcaStatistic idref="age(root)"/>
			<treeLengthStatistic idref="treeLength"/>
			<parameter idref="skygrid.precision"/>
			<parameter idref="skygrid.logPopSize"/>
			<parameter idref="skygrid.cutOff"/>
			<parameter idref="gtr.rates"/>
			<parameter idref="frequencies"/>
			<parameter idref="default.clock.rate"/>
			<parameter idref="reg.clock.rate"/>
			<rateStatistic idref="default.meanRate"/>
			<rateStatistic idref="reg.meanRate"/>

			<!-- START Ancestral state reconstruction                                    -->

			<!-- END Ancestral state reconstruction                                      -->


			<!-- START Discrete Traits Model                                             -->
			<parameter idref="reg.rates"/>
			<parameter idref="reg.indicators"/>
			<sumStatistic idref="reg.nonZeroRates"/>

			<!-- END Discrete Traits Model                                               -->

			<ancestralTreeLikelihood idref="treeLikelihood"/>
			<strictClockBranchRates idref="default.branchRates"/>
			<strictClockBranchRates idref="reg.branchRates"/>

			<!-- START Discrete Traits Model                                             -->
			<markovJumpsTreeLikelihood idref="reg.treeLikelihood"/>

			<!-- END Discrete Traits Model                                               -->

			<gmrfSkyGridLikelihood idref="skygrid"/>
		</log>

		<!-- START Discrete Traits Model                                             -->
		<log id="alpha_dis_iran_prov.regrateMatrixLog" logEvery="50000" fileName="alpha_dis_iran_prov.reg.rates.log">
			<parameter idref="reg.rates"/>
			<parameter idref="reg.indicators"/>
			<sumStatistic idref="reg.nonZeroRates"/>
		</log>
		<!-- To be manually added -->
    	<log id="completeJumpHistory" logEvery="100000" fileName="alpha_dis_iran_prov.reg_jumps_3.log">
        <completeHistoryLogger>
        <markovJumpsTreeLikelihood idref="reg.treeLikelihood"/>
        </completeHistoryLogger>
        </log> 


		<!-- END Discrete Traits Model                                               -->


		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="50000" nexusFormat="true" fileName="alpha_dis_iran_prov.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
