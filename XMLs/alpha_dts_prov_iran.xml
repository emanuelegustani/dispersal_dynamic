			<trait name="reg.states" tag="reg">
				<ancestralTreeLikelihood idref="reg.treeLikelihood"/>
			</trait>

			<!-- END Ancestral state reconstruction                                      -->

		</logTree>
			<!-- START Ancestral state reconstruction                                    -->


		<!-- write complete history tree log to file                                 -->
		<logTree logEvery="50000" nexusFormat="true" fileName="alpha_dis_iran_prov.reg.history.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<markovJumpsTreeLikelihood idref="reg.treeLikelihood"/>
