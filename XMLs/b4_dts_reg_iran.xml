	</markovJumpsTreeLikelihood>
	

	<!-- START Discrete Traits Model                                             -->

	<!-- symmetric CTMC model for discrete state reconstructions                 -->
	<generalSubstitutionModel id="region.model">
		<generalDataType idref="region.dataType"/>
