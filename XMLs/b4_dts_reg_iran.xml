		</rateIndicator>
	</generalSubstitutionModel>
	<sumStatistic id="region.nonZeroRates" elementwise="true">
		<parameter idref="region.indicators"/>
	</sumStatistic>
	<productStatistic id="region.actualRates" elementwise="false">
		<parameter idref="region.indicators"/>
		<parameter idref="region.rates"/>
	</productStatistic>
	<siteModel id="region.siteModel">
		<substitutionModel>
